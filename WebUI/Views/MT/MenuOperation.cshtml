@model IEnumerable<string>
@foreach (var links in Model)
{
    string[] link = links.Split(';');

    @Html.RouteLink(link[0], new
{
    controller = "MT",
    action = "Index",
    IDStart = link[1],
    ID = link[2]
},
            new { @class = link[2] == ViewBag.SelectedID ? "selected" : null }
)

}
